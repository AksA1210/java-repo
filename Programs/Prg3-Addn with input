
import java.util.*;
class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter first number : ");
        int a = sc.nextInt();
        System.out.print("Enter second number : ");
        int b=sc.nextInt();
        int s=a+b;
        System.out.print("Sum = "); 
        System.out.print(s); 
    }
}    
